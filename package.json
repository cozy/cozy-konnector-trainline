{
  "name": "cozy-konnector-mgen",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-mgen.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "cozy-konnector-dev src/index.js",
    "shell": "cozy-konnector-shell src/index.js",
    "standalone": "cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "test": "jest",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-trainline.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $BUILD_COMMIT --build-dir '.'"
  },
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "cozy-jobs-cli": "1.0.9",
    "cozy-konnector-libs": "3.8.1",
    "eslint-config-cozy-app": "^0.5.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-app-publish": "0.3.3",
    "eslint": "4.19.1",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  }
}
