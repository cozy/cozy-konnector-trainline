  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-app-publish": "0.3.0",
    "eslint": "4.19.1",
    "eslint-config-cozy-app": "0.5.1",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "webpack": "4.3.0",
    "webpack-cli": "2.0.13"
  }
