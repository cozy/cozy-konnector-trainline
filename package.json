{
  "name": "cozy-konnector-trainline",
  "version": "0.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-trainline.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "dependencies": {
    "cozy-konnector-libs": "3.7.4"
